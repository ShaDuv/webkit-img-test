{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "extends": "eslint:recommended",
    "env": {
      "es6": true,
      "browser": true,
      "jquery": true,
    },
      "rules": {
      "no-unused-vars": 0,
      "semi": 1,
      "indent": 0,
      "no-console": "warn",
      "no-debugger": "warn"
  }
}
